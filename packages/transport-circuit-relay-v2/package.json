{
  "name": "@devlux76/circuit-relay-v2",
  "version": "3.1.12-security-fix.1",
  "description": "Implementation of Circuit Relay v2",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/devlux76/js-libp2p/tree/main/packages/transport-circuit-relay-v2#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devlux76/js-libp2p.git"
  },
  "bugs": {
    "url": "https://github.com/devlux76/js-libp2p/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "project": true,
      "sourceType": "module"
    }
  },
  "scripts": {
    "start": "node dist/src/main.js",
    "build": "aegir build",
    "test": "aegir test",
    "clean": "aegir clean",
    "generate": "protons ./src/pb/index.proto",
    "lint": "aegir lint",
    "test:chrome": "aegir test -t browser --cov",
    "test:chrome-webworker": "aegir test -t webworker",
    "test:firefox": "aegir test -t browser -- --browser firefox",
    "test:firefox-webworker": "aegir test -t webworker -- --browser firefox",
    "test:node": "aegir test -t node --cov",
    "dep-check": "aegir dep-check",
    "doc-check": "aegir doc-check"
  },
  "dependencies": {
    "@devlux76/crypto": "^5.0.11",
    "@devlux76/interface": "^2.5.0",
    "@devlux76/interface-internal": "^2.3.0",
    "@devlux76/peer-collections": "^6.0.17",
    "@devlux76/peer-id": "^5.0.12",
    "@devlux76/peer-record": "^8.0.17",
    "@devlux76/utils": "^6.5.1",
    "@multiformats/multiaddr": "^12.3.3",
    "@multiformats/multiaddr-matcher": "^1.6.0",
    "any-signal": "^4.1.1",
    "it-protobuf-stream": "^1.1.5",
    "it-stream-types": "^2.0.2",
    "multiformats": "^13.3.1",
    "nanoid": "^5.0.9",
    "progress-events": "^1.0.1",
    "protons-runtime": "^5.5.0",
    "retimeable-signal": "^1.0.0",
    "uint8arraylist": "^2.4.8",
    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "@devlux76/interface-compliance-tests": "^6.3.0",
    "@devlux76/logger": "^5.1.8",
    "aegir": "^45.0.5",
    "delay": "^6.0.0",
    "it-drain": "^3.0.7",
    "it-pair": "^2.0.6",
    "it-pushable": "^3.2.3",
    "it-to-buffer": "^4.0.7",
    "p-wait-for": "^5.0.2",
    "protons": "^7.6.0",
    "race-signal": "^1.1.0",
    "sinon": "^19.0.2",
    "sinon-ts": "^2.0.0"
  },
  "sideEffects": false
}
