{
  "name": "@devlux76/interface",
  "version": "2.5.0-security-fix.1",
  "description": "The interface implemented by a libp2p node",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/devlux76/js-libp2p/tree/main/packages/interface#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devlux76/js-libp2p.git"
  },
  "bugs": {
    "url": "https://github.com/devlux76/js-libp2p/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "interface",
    "libp2p"
  ],
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "project": true,
      "sourceType": "module"
    }
  },
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "dep-check": "aegir dep-check",
    "doc-check": "aegir doc-check",
    "build": "aegir build"
  },
  "dependencies": {
    "@multiformats/multiaddr": "^12.3.3",
    "it-pushable": "^3.2.3",
    "it-stream-types": "^2.0.2",
    "multiformats": "^13.3.1",
    "progress-events": "^1.0.1",
    "uint8arraylist": "^2.4.8",
    "@devlux76/crypto": "*",
    "@devlux76/peer-id": "*",
    "@devlux76/logger": "*"
  },
  "devDependencies": {
    "aegir": "^45.0.5"
  },
  "browser": {
    "./dist/src/events.js": "./dist/src/events.browser.js"
  },
  "react-native": {
    "./dist/src/events.js": "./dist/src/events.browser.js"
  },
  "sideEffects": false
}
