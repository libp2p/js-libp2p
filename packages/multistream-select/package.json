    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "aegir": "^47.0.22",
    "iso-random-stream": "^2.0.2",
    "it-all": "^3.0.9",
    "it-drain": "^3.0.10",
    "it-pipe": "^3.0.1",
    "p-event": "^7.0.0",
    "p-timeout": "^7.0.0"
  },
  "sideEffects": false
}
