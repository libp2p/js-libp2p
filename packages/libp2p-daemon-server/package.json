    "pretest": "npm run build",
    "test": "aegir test -t node",
    "test:node": "aegir test -t node"
  },
  "dependencies": {
    "@libp2p/gossipsub": "^14.1.1",
    "@libp2p/crypto": "^5.1.8",
    "@libp2p/daemon-protocol": "^7.0.6",
