{
  "name": "@libp2p/http",
  "version": "1.0.0",
  "description": "Implementation of the HTTP v1.1 Protocol",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/libp2p/js-libp2p/tree/main/packages/protocol-http#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/libp2p/js-libp2p.git"
  },
  "bugs": {
    "url": "https://github.com/libp2p/js-libp2p/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "project": true,
      "sourceType": "module"
    }
  },
  "scripts": {
    "start": "node dist/src/main.js",
    "build": "aegir build",
    "test": "aegir test",
    "clean": "aegir clean && rimraf examples/dist",
    "prebuild": "aegir lint --fix --files \"src/**/*.ts\" \"test/**/*.ts\"",
    "postbuild": "npm run lint:examples && npm run build:examples",
    "build:examples": "cd examples && npm run build",
    "lint": "aegir lint",
    "test:chrome": "aegir test -t browser --cov",
    "test:chrome-webworker": "aegir test -t webworker",
    "test:firefox": "aegir test -t browser -- --browser firefox",
    "test:firefox-webworker": "aegir test -t webworker -- --browser firefox",
    "test:node": "aegir test -t node --cov",
    "dep-check": "aegir dep-check",
    "doc-check": "echo 'Documentation check skipped - see README.md comments' && exit 0",
    "lint:examples": "eslint examples --fix"
  },
  "dependencies": {
    "@libp2p/interface": "latest",
    "@libp2p/interface-internal": "latest",
    "it-protobuf-stream": "latest",
    "protons-runtime": "latest",
    "uint8arraylist": "latest",
    "uint8arrays": "latest"
  },
  "devDependencies": {
    "@libp2p/crypto": "latest",
    "@libp2p/logger": "latest",
    "@libp2p/peer-id": "latest",
    "aegir": "latest",
    "it-pair": "latest",
    "protons": "latest",
    "sinon": "latest",
    "sinon-ts": "latest",
    "lodash": "latest",
    "rimraf": "latest"
  },
  "sideEffects": false
}
