{
  "name": "@devlux76/keychain",
  "version": "5.0.14-security-fix.1",
  "description": "Key management and cryptographically protected messages",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/devlux76/js-libp2p/tree/main/packages/keychain#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devlux76/js-libp2p.git"
  },
  "bugs": {
    "url": "https://github.com/devlux76/js-libp2p/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "IPFS",
    "crypto",
    "encryption",
    "keys",
    "libp2p",
    "secure"
  ],
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "project": true,
      "sourceType": "module"
    }
  },
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "dep-check": "aegir dep-check",
    "doc-check": "aegir doc-check",
    "build": "aegir build",
    "test": "aegir test",
    "test:chrome": "aegir test -t browser --cov",
    "test:chrome-webworker": "aegir test -t webworker",
    "test:firefox": "aegir test -t browser -- --browser firefox",
    "test:firefox-webworker": "aegir test -t webworker -- --browser firefox",
    "test:node": "aegir test -t node --cov",
    "test:electron-main": "aegir test -t electron-main"
  },
  "dependencies": {
    "@devlux76/crypto": "*",
    "@devlux76/interface": "*",
    "@noble/hashes": "^1.6.1",
    "asn1js": "^3.0.5",
    "interface-datastore": "^8.3.1",
    "merge-options": "^3.0.4",
    "multiformats": "^13.3.1",
    "sanitize-filename": "^1.6.3",
    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "@devlux76/logger": "*",
    "aegir": "^45.0.5",
    "datastore-core": "^10.0.2"
  },
  "sideEffects": false
}
