  "devDependencies": {
    "@libp2p/logger": "^6.2.2",
    "@libp2p/peer-store": "^12.0.9",
    "@types/lodash.random": "^3.2.9",
    "@types/lodash.range": "^3.2.9",
    "@types/sinon": "^20.0.0",
    "@types/which": "^3.0.4",
    "aegir": "^47.0.22",
    "benchmark": "^2.1.4",
    "datastore-core": "^11.0.1",
    "delay": "^7.0.0",
    "execa": "^9.6.0",
    "it-filter": "^3.1.4",
    "it-first": "^3.0.9",
    "it-last": "^3.0.9",
    "it-length-prefixed": "^10.0.1",
    "lodash.random": "^3.2.0",
    "lodash.range": "^3.2.0",
    "p-retry": "^7.0.0",
    "protons": "^7.7.0",
    "sinon": "^21.0.0",
    "sinon-ts": "^2.0.0",
    "which": "^6.0.0"
  },
  "browser": {
    "./dist/src/routing-table/generated-prefix-list.js": "./dist/src/routing-table/generated-prefix-list-browser.js"
