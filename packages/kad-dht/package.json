    "@types/lodash.random": "^3.2.9",
    "@types/lodash.range": "^3.2.9",
    "@types/sinon": "^17.0.4",
    "@types/which": "^3.0.4",
    "aegir": "^47.0.6",
    "benchmark": "^2.1.4",
    "datastore-core": "^10.0.2",
    "delay": "^6.0.0",
    "execa": "^9.5.3",
    "it-filter": "^3.1.3",
    "it-last": "^3.0.8",
    "it-pair": "^2.0.6",
    "it-stream-types": "^2.0.2",
