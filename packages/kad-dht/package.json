    "it-map": "^3.1.3",
    "it-merge": "^3.0.11",
    "it-parallel": "^3.0.11",
    "it-pipe": "^3.0.1",
    "it-protobuf-stream": "^2.0.2",
    "it-pushable": "^3.2.3",
    "it-take": "^3.0.8",
    "mortice": "^3.0.6",
    "multiformats": "^13.3.4",
    "p-defer": "^4.0.1",
    "p-event": "^6.0.1",
