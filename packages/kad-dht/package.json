    "@libp2p/record": "^4.0.8",
    "@libp2p/utils": "^7.0.9",
    "@multiformats/multiaddr": "^13.0.1",
    "@multiformats/multiaddr-matcher": "^3.0.1",
    "any-signal": "^4.1.1",
    "interface-datastore": "^9.0.1",
    "it-all": "^3.0.9",
    "it-drain": "^3.0.10",
    "it-length": "^3.0.9",
    "it-map": "^3.1.4",
    "it-merge": "^3.0.12",
    "it-parallel": "^3.0.13",
    "it-pipe": "^3.0.1",
    "it-pushable": "^3.2.3",
    "it-take": "^3.0.9",
    "main-event": "^1.0.1",
    "multiformats": "^13.4.0",
    "p-defer": "^4.0.1",
    "p-event": "^7.0.0",
    "progress-events": "^1.0.1",
    "protons-runtime": "^5.6.0",
    "race-signal": "^2.0.0",
    "uint8-varint": "^2.0.4",
    "uint8arraylist": "^2.4.8",
    "uint8arrays": "^5.1.0"
