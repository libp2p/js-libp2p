    "it-last": "^1.0.6",
    "it-pair": "^1.0.0",
    "libp2p": "^0.34.0",
    "lodash.random": "^3.2.0",
    "lodash.range": "^3.2.0",
    "p-retry": "^4.2.0",
    "sinon": "^11.1.1",
    "which": "^2.0.2"
  },
